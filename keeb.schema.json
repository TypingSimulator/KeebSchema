{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "keebName": {
      "type": "string"
    },
    "keebDesigner": {
      "type": "string"
    },
    "workshopIDs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "grids": {
      "type": "array",
      "items": {
        "properties": {
          "id": {
            "type": "string"
          },
          "position": {
            "$ref": "#/$defs/vec3"
          },
          "rotation": {
            "$ref": "#/$defs/vec3"
          },
          "spacing": {
            "$ref": "#/$defs/vec3"
          }
        },
        "additionalProperties": false
      }
    },
    "attribution": {
      "type": "object",
      "properties": {
        "keebName": {
          "type": "string"
        },
        "authorName": {
          "type": "string"
        },
        "workshopID": {
          "type": "string"
        },
        "steamIDs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "keys": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/key"
      },
      "minItems": 1
    },
    "$schema": {
      "type": "string",
      "pattern": "https:\\\/\\\/raw\\.githubusercontent\\.com\\\/TypingSimulator\\\/KeebSchema\\\/v\\d+\\.\\d+\\.\\d+\\\/keeb\\.schema\\.json"
    }
  },
  "required": ["keebName", "keebDesigner", "keys", "$schema"],
  "additionalProperties": false,
  "$defs": {
    "key": {
      "$ref": "key.schema.json#/$defs/key"
    },
    "vec3": {
      "$ref": "key.schema.json#/$defs/vec3"
    }
  }
}
